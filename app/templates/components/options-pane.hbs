<div class="options-pane">
  <div class="options-pane__option">
    <h2>Color</h2>
    {{color-picker colorChanged=(action "colorChanged")}}
  </div>

  <div class="options-pane__option">
    <h2>Icons</h2>
    {{#items-gallery iconAdded=(action "iconAdded") rows='2' itemsOnSlide='8'}}
      {{#each icons as |icon|}}
        <div class="items-gallery__item" style='width: 25%'>
          {{ sticky-icon icon=(get iconsSvgs icon) }}
        </div>
      {{/each}}
    {{/items-gallery}}
  </div>
</div>